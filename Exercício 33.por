programa {
  funcao inicio() {
    real num1, num2, resA, resB, resC

    escreva ("Informe os números para ser efetuado o cálculo:\n")
    leia (num1, num2)
    
    resA=(num1*2)+(num2/2)
    resB=(num1*3)+num2
    resC=num2*num2*num2

    escreva ("Resultados: \nResposta A - ", resA, "\nResposta B - ", resB, "\nResposta C - ", resC)


    
  }
}
